# game/xzhong.rpy:998
translate frenchtl zhongcont_a070115e:

    # c "Besides, I could really use a souvenir."
    c "Ou simplement,l'utiliser comme des souvenirs."

# game/xzhong.rpy:1000
translate frenchtl zhongcont_5f4577a6:

    # Zh "Thank you. Thank you so much."
    Zh "Merci.Merci beaucoup."

# game/xzhong.rpy:1002
translate frenchtl zhongcont_577ecbac:

    # Zh smile b "Let me just get that for you."
    Zh smile b "Laisse moi tout te préparer"

